{
	"arrowParens": "always",
	"bracketSpacing": true,
	"printWidth": 120,
	"proseWrap": "always",
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"jsxSingleQuote": false,
	"endOfLine": "lf",
	"importOrder": ["^@(.*)$", "^[./]"],
	"importOrderSeparation": false,
	"importOrderSortSpecifiers": true,
	"plugins": ["prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"]
}
